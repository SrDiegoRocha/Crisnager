{
  "expo": {
    "name": "Crisnager",
    "slug": "Crisnager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f09ae0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.roction.crisnager"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#f09ae0"
    }
  }
}
